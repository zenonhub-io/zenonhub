{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "abraham/twitteroauth": "^5.0",
        "deployer/deployer": "^7.0",
        "digitalsloth/znn-php": "dev-main",
        "diglactic/laravel-breadcrumbs": "^8.0",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.15",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "maize-tech/laravel-markable": "dev-main",
        "marvinlabs/laravel-discord-logger": "^1.3",
        "maxmind-db/reader": "^1.11",
        "pnoeric/discourse-api-php": "^2.0",
        "psr/simple-cache": "^2.0",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-queueable-action": "^2.14",
        "spatie/laravel-short-schedule": "^1.5",
        "spatie/laravel-sitemap": "^6.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.5",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "orangehill/iseed": "^3.0",
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/digitalSloth/laravel-markable"
        }
    ],
    "autoload": {
        "files": [
            "app/Helpers/assets.php",
            "app/Helpers/zenon.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "php artisan livewire:publish --assets"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
